
@{
    ViewBag.Title = "Display";
}


<form class="woocommerce-ordering" method="get">
    <select name="orderby" class="orderby" aria-label="Shop order">
        <option value="menu_order" selected="selected">Default sorting</option>
        <option value="popularity">Sort by Status</option>
        <option value="rating">Sort by Email</option>
        <option value="date">Sort by Username</option>
    </select>
    <input type="hidden" name="paged" value="1">
</form>

<aside id="search-2" class="widget widget_search">
    <form role="search" method="get" class="search-form form-inline" action="https://colorlib.com/unite/">
        <label class="sr-only">Search for:</label>
        <div class="input-group">
            <input type="search" value="" name="s" class="search-field form-control" placeholder="filter..">
            <span class="input-group-btn">
                <button type="submit" class="search-submit btn btn-primary"><span class="glyphicon glyphicon-search"></span></button>
            </span>
        </div>
    </form>
</aside>


